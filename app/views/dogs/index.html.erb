<h1>Listing Dogs</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Toy</th>
    <th>Show</th>
    <th>Edit</th>
  </tr>

  <% @dogs.each do |dog| %>
    <tr>
      <td><%= dog.name %></td>
      <td><%= dog.age %></td>
      <td><%= dog.toy %></td>
      <td><%= link_to 'Show', dog %></td>
      <td><%= link_to 'Edit', edit_dog_path(dog) %></td>
    </tr>
  <% end %>
</table>
<%= link_to "Register New Dog", new_dog_path %> | <%= link_to 'Back', dogs_path %>