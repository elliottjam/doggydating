<h1>Your Dogs</h1>
<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>Picture</th>
    <th>Name</th>
    <th>Gender</th>
    <th>Breed</th>
    <th>Size</th>
    <th>Age</th>
    <th>Toy</th>
    <th>Show</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>

  <% @dogs.each do |dog| %>
    <tr>
      <td><%= image_tag dog.avatar.thumb.url %></td>
      <td><%= dog.name %></td>
      <td><%= dog.gender %></td>
      <td><%= dog.breed.try(:name) %></td>
      <td><%= dog.size %></td>
      <td><%= dog.age %></td>
      <td><%= dog.toy %></td>
      <td><%= link_to 'Show', dog %></td>
      <td><%= link_to 'Edit', edit_dog_path(dog) %></td>
      <td><%= link_to "Delete", dog_path(dog), method: :delete %></td>
    </tr>
  <% end %>
</table>
<%= link_to "New Dog", new_dog_path %>