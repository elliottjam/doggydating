<h1>Listing Dogs</h1>
<table>
  <tr>
    <th>Picture</th>
    <th>Name</th>
    <th>Gender</th>
    <th>Breed</th>
    <th>Size</th>
    <th>Age</th>
    <th>Toy</th>
    <th>Show</th>
    <th>Edit</th>
  </tr>

  <% @dogs.each do |dog| %>
    <tr>
      <td><%= image_tag dog.avatar.thumb.url %></td>
      <td><%= dog.name %></td>
      <td><%= dog.gender %></td>
      <td><%= dog.breed.try(:name) %></td>
      <td><%= dog.size %></td>
      <td><%= dog.age %></td>
      <td><%= dog.toy %></td>
      <td><%= link_to 'Show', dog %></td>
      <td><%= link_to 'Edit', edit_dog_path(dog) %></td>
    </tr>
  <% end %>
</table>
<%= link_to "New Dog", new_dog_path %>